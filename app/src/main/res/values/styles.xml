<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="FieldTexStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/btn_background</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">@dimen/dm_5</item>
    </style>

    <style name="MainTextStyle">
        <item name="android:fontFamily">@font/alumni_sans_medium</item>
        <item name="android:textSize">@dimen/sp_18</item>
    </style>

    <style name="TitleTextStyle" parent="MainTextStyle">
        <item name="android:textSize">@dimen/sp_24</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>